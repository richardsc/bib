@article{Chang+al:2006,
	Annote = {This paper is the butt end of criticism from Lamb 2007,
                  Scotti et al 2006, and Moum et al 2006, for not
                  including nonlinear self advection of energy in
                  their energy flux calculations. They first calculate
                  internal wave energy by computing spectra, and then
                  integrating the spectra over frequency bands of
                  interest (NLIW band, semidiurnal band, diurnal
                  band). To overcome the fact that 2 of their ADCPs
                  only covered the upper half of the water column,
                  they used normal mode analysis to compute vertically
                  integrated energies.  \\ They compute energy flux
                  two ways, \[ \vec{F} = \int \vec{C_g} E dz \] and \[
                  \vec{F} = \int <p' \vec{u}' dz \] $\vec{C_g}$ is
                  determined separately for the NLIWs (from some kind
                  of harmonic analysis?) and for the tides (from IW
                  dispersion relation and normal mode analysis). They
                  calculate $p'$ by \[ p' = p_{surf} + \int_z^0 \rho'
                  g dz \] where $\rho'$ is estimated from isopycnal
                  displacement determined from time integration of the
                  vertical velocity. Because of the large difference
                  in energy flux between 2 stations, they estimate
                  that dissipation is the cause, and calculated a
                  kinetic energy dissipation rate as \[ \varepsilon =
                  \frac{\Delta F}{\Delta s} \] To get this they take
                  the energy equation \[ \frac{\partial E}{\partial t}
                  + ( \vec{u} \cdot \nabla) E = \nabla \cdot \vec{F} -
                  \varepsilon \] and assume $\frac{\partial
                  E}{\partial t}$ and $(\vec{u} \dot \nabla) E$ are
                  small. For NLIWs this clearly is no good as the
                  previously mention critiquing papers show that the
                  self advection can account for $\sim$50\% of the
                  energy flux.},
	Author = {Chang, M.H. and Lien, R.C. and Tang, T.Y. and D'Asaro, E.A. and Yang, Y.J.},
	Journal = {Geophysical Research Letters},
	Number = {3},
	Title = {{Energy flux of nonlinear internal waves in northern South China Sea}},
	Volume = {33},
	Year = {2006}}

