	Publisher = {AGU},
	Title = {Interior versus boundary mixing of a cold intermediate layer},
	Url = {http://dx.doi.org/10.1029/2011JC007359},
	Volume = {116},
	Year = {2011},
	Bdsk-Url-1 = {http://dx.doi.org/10.1029/2011JC007359}}

