@article{Shroyer+al:2011,
	Abstract = {{Ship and mooring data collected off the coast of New
                  Jersey are used to describe the nonlinear internal
                  wave (NLIW) field and the background oceanographic
                  conditions that formed the waveguide on the
                  shelf. The subinertial, inertial, and tidal
                  circulation are described in detail, and the
                  background fluid state is characterized using the
                  coefficients of the extended Korteweg--de Vries
                  equation. The utility of this type of analysis is
                  demonstrated in description of an amplitude-limited,
                  flat wave. NLIWs observed over most of the month had
                  typical displacements of −8 m, but waves observed
                  from 17--21 August were almost twice as large with
                  displacements near −15 m. During most of the month,
                  wave packets occurred irregularly at a fixed
                  location, and often more than one packet was
                  observed per M2 tidal period. In contrast, the
                  arrival times of the large-amplitude wave groups
                  observed over 17--21 August were more closely phased
                  with the barotropic tide. The time span in which the
                  largest NLIWs were observed corresponded to neap
                  barotropic conditions, but when the shoreward
                  baroclinic energy flux was elevated. During the time
                  of large NLIWs, near-inertial waves were a dominate
                  contributor to the internal motions on the shelf and
                  apparently regulated wave formation, as
                  destructive/constructive modulation of the M2
                  internal tide by the inertial wavefield at the shelf
                  break corresponded to stronger/weaker NLIWs on the
                  shelf.  }},
	Author = {Shroyer, E. L. and Moum, J. N. and Nash, J. D.},
	Day = {23},
	Doi = {10.1029/2010JC006332},
	Issn = {0148-0227},
	Journal = {Journal of Geophysical Research},
	Month = mar,
	Number = {C3},
	Pages = {C03022+},
	Title = {{Nonlinear internal waves over New Jersey's continental shelf}},
	Url = {http://dx.doi.org/10.1029/2010JC006332},
	Volume = {116},
	Year = {2011},
	Bdsk-Url-1 = {http://dx.doi.org/10.1029/2010JC006332}}

@article{Sandstom+Elliot:2011,
	Abstract = {In the early 1980s, in situ observations were made of
                  the spatial-temporal variability of the density
                  field at three separate shelf edge sites off the
                  Canadian east coast to improve our understanding of
                  the dynamics of the generation, transformation, and
                  dissipation of internal wave energy through an
                  analysis of the potential energy budget in the water
                  column. A description of the procedure for
                  estimating the perturbation potential energy and its
                  principal characteristics is given within a
                  conceptual framework, where an internal tide is
                  generated, propagates shoreward from the shelf edge,
                  is transformed by nonlinear effects into shorter
                  undulations such as solitary waves and internal
                  bores, and eventually dissipates. On the basis of
                  observations we find that the linear approximation
                  of the potential energy is a major fraction of
                  available potential energy, especially in
                  large-amplitude internal waves. The potential energy
                  in the generated internal tide is shown to covary
                  with the squared current volume flux in the
                  generating region. Turbulence and mixing are
                  expected to occur as the short, energetic waves
                  become unstable. From the observations, the rate of
                  energy dissipation is smallest on the Grand Bank
                  (0.007 W m−2 over a 30 km distance). In the Scotian
                  Gulf the loss rate is 0.05 W m−2 over a 13 km
                  distance. At The Gully site, where the energy
                  concentrations are the largest, dissipation is
                  estimated to be as large as 0.2 W m−2 over a 10 km
                  distance.  },
	Author = {Sandstrom, H. and Elliott, J. A.},
	Doi = {10.1029/2010JC006296},
	Issn = {0148-0227},
	Journal = {Journal of Geophysical Research},
	Month = apr,
	Number = {C4},
	Pages = {C04004+},
	Title = {Production, transformation, and dissipation of energy in internal tides near the continental shelf edge},
	Url = {http://dx.doi.org/10.1029/2010JC006296},
	Volume = {116},
	Year = {2011},
	Bdsk-Url-1 = {http://dx.doi.org/10.1029/2010JC006296}}

