@unpublished{Moum+al:2007,
  author = {James N. Moum and Jody M. Klymak and Jonathan D. Nash and Alexander Perlin and William D. Smyth},
  title = {Energy transport by nonlinear internal waves},
  year = 2007,
  note = {in press, \emph{J. Phys. Oceanogr.}},
  abstract={Wintertime stratification on Oregon's continental shelf
                  often produces a near-bottom layer of dense fluid
                  that acts as an internal waveguide upon which
                  nonlinear internal waves propagate. Shipboard
                  profiling and bottom lander observations capture
                  disturbances that exhibit properties of internal
                  solitary waves, bores and gravity
                  currents. Wave-like pulses are highly turbulent
                  (instantaneous bed stresses are 1 N m^2),
                  resuspending bottom sediments into the water column
                  and raising them 30+ m above the seafloor. The waves
                  cross-shelf transport of fluid counters the
                  time-averaged Ekman transport in the bottom boundary
                  layer.  In the nonlinear internal waves we have
                  observed, the kinetic energy is roughly equal to the
                  available potential energy and is O(0.1) MJ per m of
                  coastline. The energy transported by these waves
                  includes a nonlinear advection term huEi that is
                  negligible in linear internal waves. Unlike linear
                  internal waves, the pressure-velocity energy flux
                  hupi includes important contributions from
                  nonhydrostatic effects and surface displacement. It
                  is found that, statistically, <uE> ~=
                  2<up>. Vertical profiles through these waves of
                  elevation indicate that up(z) is more important in
                  transporting energy near the seafloor while uE(z)
                  dominates farther from the bottom. With the wave
                  speed, c, estimated from weakly nonlinear wave
                  theory, it is verified experimentally that the total
                  energy transported by the waves, <up>+<uE> ~= c<E>.
                  The high but intermittent energy flux by the waves
                  is, in an averaged sense, O(100)W per m of
                  coastline.  This is similar to independent estimates
                  of the shoreward energy flux in the semidiurnal
                  internal tide at the shelfbreak.},
  annote={They have an extensive set of measurements during winter
                  stratification near bottom on the Oregon
                  Shelf. Turbulence profiles, bottom lander ADV and
                  ADCP, fixed position CTD casts, shipboard ADCP and
                  echosounder, all show aspects of ISWs of elevation
                  advancing onshelf. The observations are used to
                  evaluate nonlinear aspects of energy transport.
                  They decompose the pressure distribution associated
                  with the wave into 3 parts (see Moum and Smyth
                  2006): the internal hydrostatic pressure (from
                  isopycnal displacement), external hydrostatic
                  pressure (from free surface displacement), and a
                  nonhydrostatic pressure (from vertical
                  accelerations). Their framework gives \[
                  \frac{\partial E}{\partial t} + \frac{\partial
                  f_E}{\partial x} + \mbox{vertical redistribution} =
                  -\rho_0 \epsilon \] with the horizontal energy flux
                  as \[ f_E = u(KE + APE + p_h^w + \rho_0 g \eta_H +
                  p_{nh}) \] \[ f_E = uE + up \] where $uE$ is a
                  nonlinear advection term. They ultimately show that
                  $\langle f_E\rangle = c\langle E\rangle $ (or
                  $\langle uE + up\rangle = c\langle E\rangle $) holds
                  for the observed wave, and also that $\langle
                  uE\rangle \simeq 2\langle up\rangle $. In addition
                  they find that the nonhydrostatic and external
                  pressure contribute significantly to the total
                  pressure-velocity flux, and act to reduce it because
                  they are of the opposite sign to the hydrostatic
                  part. Also find an equipartition of KE and APE. They
                  also determine that $\rho_0 \epsilon$ is negligible
                  over short distances. Their profiling and lander
                  series do not coincide, however they focus on the
                  high res lander data for the energy
                  calculations. Also of note is that they plot
                  vertical profiles of fluxes rather than just
                  vertically integrated (fig. 15).},
  keywords={qualifying exam}
}

@article{Scotti+al:2006,
  title={{On the interpretation of energy and energy fluxes of nonlinear internal waves: an example from Massachusetts Bay}},
  author={Scotti, A. and Beardsley, R. and Butman, B.},
  journal={Journal of Fluid Mechanics},
  volume={561},
  pages={103--112},
  year={2006},
  publisher={Cambridge University Press},
  abstract={A self-consistent formalism to estimate baroclinic energy
                  densities and fluxes resulting from the propagation
                  of internal waves of arbitrary amplitude is derived
                  using the concept of available potential energy. The
                  method can be applied to numerical, laboratory or
                  field data. The total energy flux is shown to be the
                  sum of the linear energy flux $\int u'p'\,{\rm d}z$
                  (primes denote baroclinic quantities), plus
                  contributions from the non-hydrostatic pressure
                  anomaly and the self-advection of kinetic and
                  available potential energy. Using highly resolved
                  observations in Massachusetts Bay, it is shown that
                  due to the presence of nonlinear internal waves
                  periodically propagating in the area, $\int
                  u'p'\,{\rm d}z$ accounts for only half of the total
                  flux. The same data show that equipartition of
                  available potential and kinetic energy can be
                  violated, especially when the nonlinear waves begin
                  to interact with the bottom.},
  annote={Another paper highlighting the importance of evaluating
                  nonhydrostatic pressure terms, and the self
                  advection of pseudoenergy when determining energy
                  fluxes for nonlinear internal waves (either for
                  numerical, laboratory, or field data). They test the
                  theory with measurements from Massachusetts bay,
                  showing that $\int u'p' dx$ accounts for only half
                  of the total flux. Also show that the equipartition
                  of KE and APE can be violated.  The theory is
                  somewhat hard to follow, but the idea seems to be
                  that they develop their energy equation relative to
                  a reference state that is generally defined, as
                  follows. Let $\bar{\rho} = \bar{\rho}(z,t)$ be a
                  stable reference state. Then $z^*(s,t)$ is such that
                  $\bar{\rho}(z^*(s,t),t) = s$. \emph{$z^*$ is the
                  level at which a density $s$ resides in the
                  reference state????} Then \[ \psi =
                  \int_{\rho_0}^{\rho} gz^* ds \] where $\rho =
                  \rho(\mathbf{x},t)$ and $\mathrm{min}(\rho) < \rho_0
                  < \mathrm{max}(\rho)$. $\psi$ is some kind of
                  potential energy, like the potential energy at a
                  level $\rho$ referenced to $\rho_0$? Is basically a
                  generally defined reference state. $\psi$ can also
                  be written as \[ \psi = \int_{\bar{\rho}}^{\rho} g
                  z^* ds + \bar{\rho}gz - \rho_0 g z_0(t) - g
                  \int_{z_0(t)}^z\bar{\rho} dz' \] where $z_0(t) =
                  z^*(\rho_0,t)$, so that the APE is \[ E_p = \rho gz
                  - \psi.  \] \\ They also include viscous and
                  diffusive terms, which would mean that the reference
                  density state is time dependent. \emph{There is a
                  close connection here with section 2.2 of Lamb 2007,
                  and working these together may help me sort this
                  out}. Their final energy equation is: \[
                  \frac{\partial}{\partial t}(E_k + E_p) + \nabla
                  \cdot( \vec{u}(P + E_k + E_p)) = \nabla^2( \nu E_k +
                  \kappa E_p) - \epsilon_k - \epsilon_p - 2g \kappa
                  \frac{\partial}{\partial z}(\rho - \bar{\rho}) - g
                  \int_{\bar{\rho}}^{\rho} \frac{\partial
                  z^*}{\partial t} ds \] where $P$ is the deviation
                  from hydrostatic pressure distribution associated
                  with the reference state, and the terms on the right
                  are: diffusion, conversion of energy (KE and PE) to
                  internal energy (heat) through viscous friction,
                  energy used to irreversibly raise the centre of mass
                  through mixing, and changes in $E_p$ due to changes
                  in the background state. Note that \[ E_p = g
                  \int_{\bar{\rho}}^{\rho} (z - z^*) ds \] For a
                  closed system the energy equation is equivalent to
                  that of Winters et al 1995 if $\bar{\rho}$ is chosen
                  to be the adiabatic restratification of $\rho$, and
                  $E_p$ is the APE as it is commonly known.  The
                  mixing terms are globally negative, but locally can
                  have either sign.  \\Their energy equation does not
                  depend on the assumption that $\bar{\rho}$ is an
                  adiabatic restratification of $\rho$. In an open
                  system, determining the exact $\bar{\rho}$ is
                  difficult or impossible (much like Winters et al
                  1995 state), but they state: ``as long as conditions
                  within the domain allow the existence of an
                  equilibrium state ($\vec{u} = 0, \rho =
                  \bar{\rho}(z)$), one can guess a reasonable value
                  for $\bar{\rho}$, and calculate the resulting
                  fluxes." They then proceed to trash Chang et al 2006
                  for being so dumb by dropping the nonlinear flux
                  terms.  \\ The methods are applied to many (tidally
                  generated) packets of IWs. They determine
                  $\bar{\rho}$ from data taken just before arrival of
                  the waves. Equipartition is better at B than C,
                  because C is shallower. They do like Moum and Smyth
                  2006 and evaluate the hydrostatic and nonhydrostatic
                  pressure terms.  \\ They also evaluate high and low
                  passed fluxes, and find that even though the high
                  pass flux is nonzero for about 15\% of the tidal
                  cycle, it accounts for 60\% of the total flux
                  averaged over the cycle.  \\ They estimate
                  dissipation for high and low pass fluxes by
                  considering the differences between stations B and
                  C, and find that dissipation is only 2.6 times
                  greater as a result of the waves.},
  keywords={qualifying exam, energy flux, nonlinear internal waves}
}

@article{Lamb:2003,
  title={{Shoaling solitary internal waves: on a criterion for the formation of waves with trapped cores}},
  author={Lamb, K.G.},
  journal={Journal of Fluid Mechanics},
  volume={478},
  pages={81--100},
  year={2003},
  publisher={Cambridge Univ Press},
  abstract={Shoaling solitary internal waves are ubiquitous features
                  in the coastal regions of the world's oceans where
                  waves with a core of recirculating fluid (trapped
                  cores) can provide an effective transport
                  mechanism. Here, numerical evidence is presented
                  which suggests that there is a close connection
                  between the limiting behaviour of large-amplitude
                  solitary waves and the formation of such waves via
                  shoaling. For some background states,
                  large-amplitude waves are broad, having a nearly
                  horizontal flow in their centre. The flow in the
                  centre of such waves is called a conjugate flow. For
                  other background states, large-amplitude waves can
                  reach the breaking limit, at which the maximum
                  current in the wave is equal to the wave's
                  propagation speed. The presence of a background
                  current with near-surface vorticity of the same sign
                  as that induced by the wave can change the limiting
                  behaviour from the conjugate-flow limit to the
                  breaking limit. Numerical evidence is presented here
                  which suggests that if large solitary waves cannot
                  reach the breaking limit in the shallow water, that
                  is if the background flow has a conjugate flow, then
                  waves with trapped cores will not be formed via
                  shoaling. It is also shown that, due to a change in
                  the limiting behaviour of large waves, an
                  appropriate background current can enable the
                  formation of waves with trapped cores in
                  stratifications for which such waves are not formed
                  in the absence of a background current.},
  annote={An extension of the numerical experiments in Lamb 2002,
                  extended to look at oceanographically relevant
                  stratifications (i.e. tanh profiles), and including
                  background current shear near the surface. In Lamb
                  2002 he found that trapped cores would not form when
                  there was a surface mixed layer, and the maximum
                  amplitude of the waves was governed by the conjugate
                  flow limit ($U_{max} < c$). In the intro he states
                  \\ \emph{``The reason for the significant difference
                  in the limiting behavior of large waves ... is that
                  in the mixed layer there is no mechanism for the
                  generation of vorticity."}  \\This suggests however,
                  that a surface current with vorticity in the same
                  direction can enable the formation of trapped cores
                  in a stratification that otherwise wouldn't support
                  it.  \\Lamb and Wan 1998 found that conjugate flows
                  with open streaklines (i.e. not trapped cores) exist
                  only when the centre of the pycnocline is greater
                  than a critical distance $r(d)$ from the upper (wave
                  of depression) or lower (waves of elevation) boundary
                  ($d$ is the pycnocline thickness).  \\The results
                  with no background current are summarized in figure
                  3, where trapped cores only form if $z_0$
                  (pycnocline depth) is greater than $r_u(d)$ for
                  various values of $d$. In all cases shoaling waves
                  steepen at the back and if of sufficient amplitude
                  they overturn. For conjugate flow limited waves
                  ($z_0 < r_u(d)$) a patch of surf formed at the rear
                  of the wave during overturning is left behind, and
                  waves with amplitudes larger than the limit flatten
                  out from the front (see figure 4a). If $z_0$ is
                  above $r_u(d)$ there is a persistent surf near the
                  rear, and for $z_0 > r_u(d)$ the closed density
                  contours fill more of the wave.  \\Adding a sheared
                  background current modifies the $r_u(d)$ curve
                  (figure 6), but it is still found that trapped cores
                  only form when $z_0 > r_u(d)$. Now however, $r_u(d)$
                  is generally lower than with no current, meaning
                  trapped cores can be formed for stratifications that
                  couldn't previously. In other words, as the
                  background shear increases, $r_u(d)$ decreases.
                  \\He closes by stating: \emph{``The numerical
                  calculation of conjugate flows for a specified
                  stratification and background current is
                  computationally very cheap and provides a simple
                  means to determine whether waves with trapped cores
                  can exist."} How true it is.},
  keywords={qualifying exam}
}

@article{Brown+Sutherland:2007,
  title={{Internal Wave Tunnelling Through Non-uniformly Stratified Shear Flow}},
  author={Brown, G.L. and Sutherland, B.R.},
  journal=ao,
  volume={45},
  number={1},
  pages={47--56},
  year={2007},
  publisher={Canadian Meteorological and Oceanographic Society},
  abstract={We examine the transmission of internal gravity waves
                  through a non-uniformly stratified fluid with
                  vertically varying background shear. To quantify
                  wave transmission we show that the appropriate
                  measure is the ratio of the flux of transmitted to
                  incident pseudoenergy, T. We derive an analytic
                  prediction of T for the transmission of waves
                  through a piecewise-linear shear flow in two
                  cases. In both, the fluid is unstratified over the
                  depth of the shear and uniformly stratified
                  elsewhere.  In one study, the density profile is
                  continuous. Such a basic state is unstable but with
                  vanishingly small growth rate as the bulk Richardson
                  number, Ri, becomes large. In the limit of an
                  infinitely large Richardson number (no shear), we
                  recover the tunnelling prediction of Sutherland and
                  Yewchuk (2004). In weak shear, incident waves can
                  transmit weakly, even if the phase speed matches the
                  flow speed within the shear layer (a critical
                  level).  However, no transmission occurs when the
                  phase speed of incident waves exactly matches the
                  flow speed on the opposite flank of the shear. In
                  strong shear, with Ri $\simeq$ 1, a transmission
                  peak occurs where the incident wavenumber and
                  frequency are close to, but different from, those
                  associated with unstable modes.  In a second study,
                  the background density profile is discontinuous and
                  representative of a well-mixed patch within a once
                  uniformly stratified fluid. In this case, no
                  transmission occurs for incident waves with phase
                  speed matching the speed of the flow within the
                  shear layer. However, a transmission spike occurs if
                  the incident waves are resonant with interfacial
                  waves that flank the shear.},
  annote={They examine the transmission of IWs through an ``$N^2$
                  barrier" and a ``mixed $N^2$ region when there is
                  shear present. Essentially an extension of
                  Sutherland and Yewchuck (2004) which didn't include
                  shear. The addition of shear requires some stability
                  consideration, though they find the timescales for
                  instability are longer than those for wave
                  transmission through the barrier. Presence of shear
                  also requires consideration of ``Pseudoenergy"
                  instead of energy, due to interaction of the waves
                  and the flow. Pseudoenergy is conserved (it is
                  proportional to the action) and is given by the
                  energy multiplied by $\omega / \Omega$, where
                  $\Omega$ is the relative frequency given by
                  $\Omega(z) = \omega - kU(z)$. \emph{How does this
                  pseudoenergy relate to KE + APE??}  \\ {\bf More
                  notes:} The presence of shear means that tunneling
                  internal waves also have to contend with a critical
                  level (where the mean current is equal to the
                  horizontal wave speed, so that $\Omega(z) = 0$). As
                  long as the horizontal phase speed of the waves
                  matches the flow speed in the upper layer, there is
                  no transmission (obviously). If the critical level
                  occurs in the region where $N =0$ though, the waves
                  may transmit through it. The reason the waves can
                  cross the critical level is because they are
                  evanescent in the shear region, not wave-like. In
                  fact, they find nearly perfect transmission for $kL
                  \leq 2$ and $\omega \leq N_0$ if $Ri = 1$.
                  \\For the mixed $N^2$ barrier (when $N^2$ is
                  discontinuous), no waves are transmitted when $0 < c
                  < U_0$, however there is much more transmission
                  otherwise, as well as a strong transmission spike at
                  $\omega \leq N_0$ and $kL \simeq 2.4$. In other
                  words, the presence of density discontinuities
                  serves to enhance transmission when no critical
                  level is reached, but completely inhibits it
                  otherwise.},
  keywords={qualifying exam}
}

@article{Kunze+al:2002,
  title={{Internal waves in Monterey Submarine Canyon}},
  author={Kunze, E. and Rosenfeld, L.K. and Carter, G.S. and Gregg,
                  M.C.},
  journal={Journal of Physical Oceanography},
  volume={32},
  number={6},
  pages={1890--1913},
  year={2002},
  publisher={American Meteorological Society},
  abstract={Velocity, temperature, and salinity profile surveying in
                  Monterey Submarine Canyon during spring tide reveals
                  an internal wave field almost an order of magnitude
                  more energetic than that in the open
                  ocean. Semidiurnal fluctuations and their harmonics
                  dominate, near-inertial motions are absent. The
                  ratio of horizontal kinetic to available potential
                  energy is less than one in much of the canyon,
                  inconsistent with hydrostatic internal waves. The
                  excess potential energy may be due to isopycnal
                  displacements induced by barotropic tide flow over
                  the sloping bottom. Removal of the expected
                  barotropic contribution raises the energy ratio to
                  2.04-2.10, in line with the semidiurnal internal
                  wave value of 2.13. Finescale shear and strain are
                  also elevated. Finescale parameterizations for
                  turbulent eddy diffusivities, which have proven
                  successful in the open ocean, underestimate
                  upper-canyon microstructure estimates of 100 $\times
                  10^4$ m$^2$ s$^{-1}$ by a factor of 30. Energy
                  fluxes and near-bottom velocities are strongly
                  steered by the sinuous canyon topography. A
                  vertically integrated influx of 5 kW m$^{-1}$ at the
                  mouth diminishes to $\pm$1 kW m$^{-1}$ toward the
                  shallow end of the canyon. Both sinks and sources of
                  internal wave energy are indicated by energy-flux
                  convergences and divergences along the canyon
                  axis. Along-axis energy-flux convergences are
                  consistent with microstructure dissipation rates
                  $\varepsilon$. The high diapycnal eddy diffusivities
                  may drive strong nutrient fluxes to enhance
                  bioproductivity.},
  annote={The authors examine the internal wave field in a submarine
                  canyon, using XCP-XCTD pairs, bottom mounted ADCPs,
                  and CTD time series. Canyons are often areas of
                  increased biological productivity, possibly due to
                  upwelling and subsequent upcanyon flow. Enhanced
                  turbulence from an enhanced internal wave field may
                  also contribute. Canyons may contribute to higher
                  turbulence through focusing of internal waves and
                  critical reflection off the bottom.  \\ Sampling
                  included across canyon transects, repeated four
                  times in 12h, an along canyon transect, and various
                  measurements on the submarine fan and slope. ADCPs
                  were mounted near the shallow end of the
                  canyon. Isopycnal displacement ($\xi$) was
                  calculated from XCTD/XCP data relative to averages
                  over the entire survey. Energy fluxes were then
                  determined from $\langle \tilde{u} \tilde{p}
                  \rangle$, where $\tilde{p}$ was calculated from the
                  survey averaged $N^2$ and isopycnal profiles
                  assuming a hydrostatic balance. They note that this
                  holds for frequencies $\omega \ll N$, and is
                  therefore appropriate for the mostly semidiurnal
                  fluctuations.  \\ They produce frequency spectra
                  from the ADCP. For $f < \omega < N$ it resembles the
                  GM1979 spectrum except for: there is no inertial
                  peak, a dominant semidiurnal peak, the presence of
                  M2 and K1 harmonics, and excess energy near $N$
                  (probably caused by instrument noise). They
                  calculate KE and APE along and across the canyon,
                  using \[ PE = \frac{\bar{N}^2 \langle \xi^2
                  \rangle}{2} \] but to remove the barotropic
                  component they subtract a linear fit for $\xi$ (with
                  zero at the surface) from the isopycnal
                  displacements (i.e. $\tilde{\xi} = \xi - \xi_z
                  z$). Not doing this can lead to KE/PE ratios that
                  are too low. There are a number of plots showing the
                  energy along and across the canyon, showing
                  baroclinic energies are elevated compared to
                  open-ocean values, especially toward the head of the
                  canyon.  \\They also estimate finescale shear and
                  strain variance, which they use for estimates of
                  turbulent diffusion using several different
                  parameterizations. They find they best
                  parameterization underestimates microstructure
                  measurements by a factor of 30, implying that
                  open-ocean parameterizations are not valid in the
                  canyon, and that turbulence there is produced in a
                  fundamentally different way (either through enhanced
                  wave-wave interactions, the departure from the GM
                  spectrum, or interaction with canyon topography).
                  \\They estimate flux magnitudes and direction,
                  finding that they are mostly oriented up-canyon, and
                  in some cases seem to be able to navigate bends with
                  little loss. They then combine the flux and
                  dissipation rate to see whether the loss of energy
                  goes to turbulence (using steady state and weak
                  advection assumptions) i.e.  \[ \frac{\Delta \langle
                  \tilde{u}\tilde{p} \rangle}{\Delta r} =
                  \varepsilon_p = \varepsilon(1+\gamma) \] where
                  $\gamma$ is the mixing efficiency. They find that
                  flux convergences are comparable to $\varepsilon_p$,
                  though it is not just simple upcanyon dissipation as
                  there must be sources and sinks to explain the
                  observed flux. This implies that if internal tide
                  generation is important in the canyon, that might
                  control turbulence rather than an internal wave
                  cascade.},
  keywords={qualifying exam}
}

@article{Alford:2003,
  title={{Redistribution of energy available for ocean mixing by
                  long-range propagation of internal waves.}},
  author={Alford, MH},
  journal={Nature},
  volume={423},
  number={6936},
  pages={159--62},
  year={2003},
  abstract={Ocean mixing, which affects pollutant dispersal, marine
                  productivity and global climate, largely results
                  from the breaking of internal gravity
                  waves--disturbances propagating along the ocean's
                  internal stratification. A global map of
                  internal-wave dissipation would be useful in
                  improving climate models, but would require
                  knowledge of the sources of internal gravity waves
                  and their propagation. Towards this goal, I present
                  here computations of horizontal internal-wave
                  propagation from 60 historical moorings and relate
                  them to the source terms of internal waves as
                  computed previously. Analysis of the two most
                  energetic frequency ranges--near-inertial
                  frequencies and semidiurnal tidal
                  frequencies--reveals that the fluxes in both
                  frequency bands are of the order of 1 kW x m(-1)
                  (that is, 15-50\% of the energy input) and are
                  directed away from their respective source
                  regions. However, the energy flux due to
                  near-inertial waves is stronger in winter, whereas
                  the tidal fluxes are uniform throughout the
                  year. Both varieties of internal waves can thus
                  significantly affect the space-time distribution of
                  energy available for global mixing.},
  annote={Alford presents an attempt to map energy fluxes in the
                  internal wave field for the inertial and semidiurnal
                  bands. Previous studies (Munk and Wunsch 1998,
                  Wunsch 1998) have shown that the wind and tide input
                  enough power ($\sim$ 2 TW) to maintain abyssal
                  stratification, however, long range propagation of
                  energy will produce significant differences between
                  source and dissipation regions, which with perfect
                  knowledge of sources and horizontal flux is given by
                  $D({\bf x}) = S({\bf x}) - \nabla \cdot {\bf
                  F(x)}$. Here he uses historical data at 60 locations
                  to compute flux magnitudes and directions for near
                  inertial and tidal frequencies. Near inertial fluxes
                  are greater in winter, and are predominantly
                  directed equatorward, while M$_2$ fluxes radiate
                  away from known source regions and are steady
                  throughout the year. Alford estimates the
                  magnitudes of fluxes in some of the regions, and
                  determines that globally significant amounts of
                  energy are being transported across
                  basins. E.g. $\sim$ 12 GW of inertial energy
                  produced in the northern pacific propagates into the
                  south pacific where if dissipated uniformly would
                  yield a depth mean dissipation rate of 10$^{-10}$ W
                  kg$^{-1}$ (typical of background thermocline mixing
                  rates). Similarly, calculations near the west
                  pacific trench and Hawaiian ridge indicate that much
                  of the internal tidal energy leaves the source
                  region ($\sim$50\% and 30\% respectively).},
  keywords={qualifying exam}
}

@Article{Lamb:2007,
  author = 	 {Lamb, KG},
  title = 	 {Energy and pseudoenergy flux in the internal wave
                  field generated by tidal flow over topography},
  journal = 	 csr,
  year = 	 {2007},
  volume = 	 {27},
  number = 	 {9},
  pages = 	 {1208--1232},
  abstract =     {},
  annote = 	 {Lamb examines aspects of tidal flow over topography
                  using a nonlinear nonhydrostatic 2D numerical
                  model. He examines 2 cases: weak tidal flow with
                  constant N over a deep ocean ridge, and strong tidal
                  flow over a bank edge with nonuniform
                  stratification. He evaluates the fluxes of total
                  mechanical energy and pseudoenergy for the cases,
                  while evaluating contributions from various flux
                  terms, including: flux of background and
                  perturbation potential energy, rate of work done by
                  background and perturbation pressure fields, and
                  flux of kinetic energy. (Note that he points out
                  that the perturbation pressure field may further be
                  decomposed into hydrostatic and nonhydrostatic
                  parts, following Moum et al 2006 and Venayagamoorthy
                  and Fringer 2005, 2006).  The total mechanical
                  energy equation is of the form: \[ \frac{d
                  \bar{E}}{dt} = \left. \left[ (PE_{f1} + W_1) +
                  PE_{f2} + W_2 + K_f \right] \right|_{\xi_r}^{\xi_l}
                  \] and the pseudoenergy equation is: \[ \frac{d
                  \bar{E}_{pseudo}}{dt} = \left. (K_f + APE_f + W_2)
                  \right|_{\xi_r}^{\xi_l}.  \] Lamb finds that both
                  equations are satisfied to less than 1\%, indicating
                  that using a linear energy equation
                  (i.e. considering only the pressure work terms in
                  the flux) is not appropriate for the bank edge
                  case. In fact, the $\langle up \rangle$ term accounts for only
                  $\sim$1/2 of the pseudoenergy flux. This is
                  consistent with other studies, in particular Moum et
                  al 2006 and Scotti et al 2006, and reinforces that
                  much of the internal wave energy generated by
                  tide-topography interaction ends up in the high
                  frequency nonhydrostatic ISW field.  },
  keywords = {qualifying exam}
}

