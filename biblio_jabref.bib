@ARTICLE{Alford:2003,
  author = {Alford, MH},
  title = {{Redistribution of energy available for ocean mixing by long-range
	propagation of internal waves.}},
  journal = {Nature},
  year = {2003},
  volume = {423},
  pages = {159--62},
  number = {6936},
  abstract = {Ocean mixing, which affects pollutant dispersal, marine productivity
	and global climate, largely results from the breaking of internal
	gravity waves--disturbances propagating along the ocean's internal
	stratification. A global map of internal-wave dissipation would be
	useful in improving climate models, but would require knowledge of
	the sources of internal gravity waves and their propagation. Towards
	this goal, I present here computations of horizontal internal-wave
	propagation from 60 historical moorings and relate them to the source
	terms of internal waves as computed previously. Analysis of the two
	most energetic frequency ranges--near-inertial frequencies and semidiurnal
	tidal frequencies--reveals that the fluxes in both frequency bands
	are of the order of 1 kW x m(-1) (that is, 15-50\% of the energy
	input) and are directed away from their respective source regions.
	However, the energy flux due to near-inertial waves is stronger in
	winter, whereas the tidal fluxes are uniform throughout the year.
	Both varieties of internal waves can thus significantly affect the
	space-time distribution of energy available for global mixing.},
  annote = {Alford presents an attempt to map energy fluxes in the internal wave
	field for the inertial and semidiurnal bands. Previous studies (Munk
	and Wunsch 1998, Wunsch 1998) have shown that the wind and tide input
	enough power ($\sim$ 2 TW) to maintain abyssal stratification, however,
	long range propagation of energy will produce significant differences
	between source and dissipation regions, which with perfect knowledge
	of sources and horizontal flux is given by $D({\bf x}) = S({\bf x})
	- \nabla \cdot {\bf F(x)}$. Here he uses historical data at 60 locations
	to compute flux magnitudes and directions for near inertial and tidal
	frequencies. Near inertial fluxes are greater in winter, and are
	predominantly directed equatorward, while M$_2$ fluxes radiate away
	from known source regions and are steady throughout the year. Alford
	estimates the magnitudes of fluxes in some of the regions, and determines
	that globally significant amounts of energy are being transported
	across basins. E.g. $\sim$ 12 GW of inertial energy produced in the
	northern pacific propagates into the south pacific where if dissipated
	uniformly would yield a depth mean dissipation rate of 10$^{-10}$
	W kg$^{-1}$ (typical of background thermocline mixing rates). Similarly,
	calculations near the west pacific trench and Hawaiian ridge indicate
	that much of the internal tidal energy leaves the source region ($\sim$50\%
	and 30\% respectively).},
  keywords = {qualifying exam}
}

